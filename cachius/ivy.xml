<ivy-module version="1.3">
    <info organisation="riotfamily" module="cachius" revision="${riot.revision}" />
    <configurations>
    	<conf name="build" />
    	<conf name="runtime" />
    	<conf name="test" extends="runtime" />
    </configurations>
    <publications>
    	<artifact />
    	<artifact type="source" ext="src.zip" />
    </publications>
    <dependencies>
		<dependency org="apache" name="commons-logging" rev="${dep.commons-logging.revision}" conf="build,runtime->default" />
		<dependency org="sun" name="servlet-api" rev="2.4" conf="build->default" />
    </dependencies>
</ivy-module>
